  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "yarn run lint --fix",
    "postinstall": "cd .. && husky install frontend/.husky",
    "generate": "graphql-codegen --config codegen.yml",
    "test": "cypress open",
    "test:headless": "cypress run",
