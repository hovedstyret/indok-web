    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.8.3",
    "@sentry/nextjs": "^7.1.1",
    "dayjs": "^1.10.5",
    "deepmerge": "^4.2.2",
    "formik": "^2.2.9",
