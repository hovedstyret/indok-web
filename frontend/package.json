    "*.{json,md,html,js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "../backend/**/*.py": [
      "black",
      "flake8 --config=../backend/tox.ini"
    ],
    "../backend/schema.json": [
      "yarn generate"
    ]
