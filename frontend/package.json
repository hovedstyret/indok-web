    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.11.12",
    "@sentry/nextjs": "^7.44.1",
    "@vercel/analytics": "^1.0.0",
    "dayjs": "^1.11.7",
    "deepmerge": "^4.2.2",
    "graphql": "^16.3.0",
