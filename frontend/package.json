  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typed-document-node": "^2.3.5",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-typed-document-node/core": "^3.2.0",
